<div class="row">
    <div class="col-md-10">
        <h2>Search Results</h2>
    </div>
</div>

<%= will_paginate @athletes, renderer: BootstrapPagination::Rails %>
<div class="row search-result">
    <% @athletes.each do |athlete| %>
    <div class='col-md-10 well member col-md-offset-2'>
        <%= athlete.full_name %>
    </div>
    <% end %>
</div>